<script>
    function saveInboundSettings() {
        var isssl = false;
        if ($("#iismailssl").is(':checked')) {
            isssl = true;
        }
        else{
            isssl = false;
        }
        if ($("#imailHostUrl").val().trim() == "") {
            $("#imailHostUrl").addClass('invalid');
        }
        else if ($("#imailusername").val().trim() == "") {
            $("#imailusername").addClass('invalid');
            $("#imailHostUrl").removeClass('invalid');
        }
        else if ($("#imailpassword").val().trim() == "") {
            $("#imailpassword").addClass('invalid');
            $("#imailusername").removeClass('invalid');
            $("#imailHostUrl").removeClass('invalid');
        }
        else if ($("#imailport").val().trim() == "") {
            $("#imailport").addClass('invalid');
            $("#imailpassword").removeClass('invalid');
            $("#imailusername").removeClass('invalid');
            $("#imailHostUrl").removeClass('invalid');
        }
        else {
            $("#imailport").removeClass('invalid');
            $("#imailpassword").removeClass('invalid');
            $("#imailusername").removeClass('invalid');
            $("#imailHostUrl").removeClass('invalid');

            $(".se-pre-con").show();
            $.ajax({
                url: '/UserSetings/AddMailSetings',
                dataType: "html",
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({ hosttype: $("#imailHost").val(), hosturl: $("#imailHostUrl").val(), port: $("#imailport").val(), username: $("#imailusername").val(), password: $("#imailpassword").val(), isssl: isssl, msettingId: $("#imailsettingId").val(),settingtype:'INBOUND' }),
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data != '') {           
                        $("#isuccessdiv").show();
                        $("#ieroordiv").hide();
                        $("#imailsettingId").val(data);
                        $(".se-pre-con").hide();
                    }
                    else {
                        $("#ieroordiv").show();
                        $("#isuccessdiv").hide();
                        $(".se-pre-con").hide();
                        
                    }
                },
                error: function (xhr) {
                    alert();
                    $('#errorMessage').html("No connection");
                    $(".se-pre-con").hide();
                }
            });
        }
    }

