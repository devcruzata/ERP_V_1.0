        else {
            isssl = false;
        }
        if ($("#omailHostUrl").val().trim() == "") {
            $("#omailHostUrl").addClass('invalid');
        }
        else if ($("#omailusername").val().trim() == "") {
            $("#omailusername").addClass('invalid');
            $("#omailHostUrl").removeClass('invalid');
        }
        else if ($("#omailpassword").val().trim() == "") {
            $("#omailpassword").addClass('invalid');
            $("#omailusername").removeClass('invalid');
            $("#omailHostUrl").removeClass('invalid');
        }
        else if ($("#omailport").val().trim() == "") {
            $("#omailport").addClass('invalid');
            $("#omailpassword").removeClass('invalid');
            $("#omailusername").removeClass('invalid');
            $("#omailHostUrl").removeClass('invalid');
        }
        else {
            $("#omailport").removeClass('invalid');
            $("#omailpassword").removeClass('invalid');
            $("#omailusername").removeClass('invalid');
            $("#omailHostUrl").removeClass('invalid');

            $(".se-pre-con").show();
            $.ajax({
                url: '/UserSetings/AddMailSetings',
                dataType: "html",
                type: "POST",
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify({ hosttype: 'SMTP', hosturl: $("#omailHostUrl").val(), port: $("#omailport").val(), username: $("#omailusername").val(), password: $("#omailpassword").val(), isssl: isssl, msettingId: $("#omailsettingId").val(), settingtype: 'OUTBOUND' }),
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data != '') {
                        $("#osuccessdiv").show();
                        $("#oeroordiv").hide();
                        $("#omailsettingId").val(data);
                        $(".se-pre-con").hide();
                    }
                    else {
                        $("#oeroordiv").show();
                        $("#osuccessdiv").hide();
                        $(".se-pre-con").hide();
                    }
                },
                error: function (xhr) {
                    alert();
                    $('#errorMessage').html("No connection");
                    $(".se-pre-con").hide();
                }
            });
        }
    }

    function GetMailSetings() {
        $(".se-pre-con").show();
        $.ajax({
            url: '/UserSetings/GetMailSetings',
            //dataType: "html",
            type: "POST",
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({}),
            async: true,
            processData: false,
            cache: false,
            success: function (data) {
                if (data != '') {
                    $("#imailsettingId").val(data.inMailSeting_Id_Pk);
                    $("#imailHost").val(data.inHostType);
                    $("#imailHostUrl").val(data.inHostUrl);
                    $("#imailport").val(data.inPort);
                    $("#imailusername").val(data.inUsername);
                    $("#imailpassword").val(data.inPassword);
                    if (data.inIsssl == true) {
                        $('#iismailssl').prop('checked', true);
                    }
                    $("#omailsettingId").val(data.oMailSeting_Id_Pk);
                    $("#omailHostUrl").val(data.oHostUrl);
                    $("#omailport").val(data.oPort);
                    $("#omailusername").val(data.oUsername);
                    $("#omailpassword").val(data.oPassword);
                    if (data.oIsssl == true) {
                        $('#oismailssl').prop('checked', true);
                    }
                    $(".se-pre-con").hide();
                }
                else {                    
                    $(".se-pre-con").hide();
                }
            },
            error: function (xhr) {               
                $('#errorMessage').html("No connection");
                $(".se-pre-con").hide();
            }
        });
    }
</script>








