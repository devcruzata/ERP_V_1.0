<script>
    $(document).ready(function () {
        $("#edit_btn").click(function () {
            $("#viewForm").hide();
            $("#edit_btn").hide();
            $("#save_btn").show();
            $("#cancel_btn").show();
            $("#mainForm").show();
        });

        $("#cancel_btn").click(function () {
            $("#mainForm").hide();
