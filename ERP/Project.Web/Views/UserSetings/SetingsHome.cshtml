<div tabindex="-1" role="dialog" aria-labelledby="myAnimationModalLabel" class="modal animated fadeInLeft bs-example-modal-animation" id="DropListModal">
    <div role="document" class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-label="Close" class="close"><span aria-hidden="true">×</span></button>
                <h4 id="myAnimationModalLabel" class="modal-title">Manage Droplist Option</h4>
            </div>
            <div class="modal-body">
                <form action="/UserSetings/AjaxManageSource" method="post" id="dropListForm">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="form-group col-sm-3">
                            </div>
                            <div class="form-group col-sm-6">
                                <input type="text" id="Option" name="Option" class="form-control form-cutom" placeholder="Option Text" />
                                <div id="error_div_Option" style="display:none;">
                                    <p style="color:red">Please Enter Option Text</p>
                                </div>
                            </div>
                            <div class="form-group col-sm-3">
                            </div>
                        </div>
                    </div>
                    <input type="hidden" name="OptionID" id="OptionID" />
                    <input type="hidden" name="hModule" id="hModule" />
                    <input type="hidden" name="hdroplist" id="hdroplist" />
                </form>

            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="btn btn-raised btn-default" id="cancel"onclick="CancelDroplistOtion()">Cancel</button>
                <button type="button" class="btn btn-raised btn-danger" id="save" onclick="AddDropListOption();">Save</button>
            </div>
        </div>
    </div>
</div>

<div tabindex="-1" role="dialog" aria-labelledby="myAnimationModalLabel" class="modal animated fadeInLeft bs-example-modal-animation" id="RolesModal">
