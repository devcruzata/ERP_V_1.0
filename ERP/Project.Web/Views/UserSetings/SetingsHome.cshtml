            $("#cancel_btn").hide();
            $("#edit_btn").show();
            $("#viewForm").show();            
            
        });
    });

    function SubmitForm() {       
        var formPayment = $('#mainForm');
            $.ajax({
                url: formPayment.attr('action'),
                type: "POST",
                data: formPayment.serialize(),
                async: true,
                processData: false,
                cache: false,
                success: function (data) {
                    if (data != '') {                        
                        $("#mainForm").hide();
                        $("#save_btn").hide();
                        $("#cancel_btn").hide();
                        $("#edit_btn").show();
                        $("#viewForm").show();
                        $("#viewForm").html('');
                        $("#viewForm").html(data);
                    }
                    else {
                        location.reload();
                    }
                },
                error: function (xhr) {
                    $('#errorMessage').html("No connection");
                }
            });
        
    }
</script>


@* Droplist Seting Scripts****** *@
<script>   

    $(document).ready(function () {

        //For Module Chage
        $("#module").change(function () {
            if ($("#module").val() == "Lead") {
                $("#dropList1").show();
                $("#dropList2").hide();
                $("#dropList3").hide();
                $("#OptionsBox").hide();
            }
            else if ($("#module").val() == "Opportunity") {
                $("#dropList1").hide();
                $("#dropList2").show();
                $("#dropList3").hide();
                $("#OptionsBox").hide();

            }
            else if ($("#module").val() == "Client") {
                $("#dropList1").hide();
                $("#dropList2").hide();
                $("#dropList3").show();
                $("#OptionsBox").hide();
            }
            else {
                $("#dropList1").hide();
                $("#dropList2").hide();
                $("#dropList3").hide();
                $("#OptionsBox").hide();

            }
        });

        //For Lead Pages
        $("#droplistSelect1").change(function () {
            GetDroplistOptions('Lead', $("#droplistSelect1").val());

        });

        //For Opportunity Pages
        $("#droplistSelect2").change(function () {
            GetDroplistOptions('Opportunity', $("#droplistSelect2").val());

        });

        //For Client Pages
        $("#droplistSelect3").change(function () {
            GetDroplistOptions('Client', $("#droplistSelect3").val());


        });


    });

    function GetDroplistOptions(Module, dName) {

        $.ajax({
            url: '/UserSetings/GetDropListing',
            dataType: "html",
            type: "POST",
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({ Module: Module, DropName: dName }),
            async: true,
            processData: false,
            cache: false,
            success: function (data) {               
                if (data != '') {                   
                    $("#pbody").html('');
                    $("#pbody").html(data);
                    $('#hModule').val(Module);
                    $('#hdroplist').val(dName);
                    $('#Option').val('');
                    $('#OptionID').val('');
                    $("#OptionsBox").show();
                   
                }
                else {
                    $('#Option').val('');
                    $('#OptionID').val('');
                    $("#OptionsBox").show();
                }
            },
            error: function (xhr) {
                alert();
                $('#errorMessage').html("No connection");
            }
        });        
    }

    function AddDropListOption() {
        if ($('#Option').val().trim() == '') {
            $('#error_div_Option').show();
        }
        else {
            $('#error_div_Option').hide();
            $.post('/UserSetings/AjaxManageDropListOption', { OptionText: $('#Option').val(), OptionID: $('#OptionID').val(), Module: $('#hModule').val(), DropListName: $('#hdroplist').val() }, function (data) {
                if (data != '') {
                    $("#pbody").html('');
                    $("#pbody").html(data);
                    $('#DropListModal').modal('hide');
                    $("#dropListForm")[0].reset();
                }
            });
        }
    }
    function CancelDroplistOtion()
    {
        $('#Option').val('');
        $('#error_div_Option').hide();
    }

    function EditOption(OptionID, OptionText) {
        $("#Option").val(OptionText);
        $("#OptionID").val(OptionID);
    }

    function DeleteOption(Op_ID) {
        swal({
            title: "Are you sure?",
            text: "Are you sure to delete this Option!",
            type: "warning",
            showCancelButton: !0,
            cancelButtonClass: "btn-raised btn-default",
            cancelButtonText: "Cancel!",
            confirmButtonClass: "btn-raised btn-danger",
            confirmButtonText: "Yes, delete it!",
            closeOnConfirm: !0
        },
        function () {
            $.post('/UserSetings/DeleteDropListOption', { OptionID: Op_ID, Module: $('#hModule').val(), DropListName: $('#hdroplist').val() }, function (data) {
                if (data != "") {
                    $("#pbody").html('');
                    $("#pbody").html(data);
                }
                else {
                    location.reload();
                }
            });
        })
    }

</script>


